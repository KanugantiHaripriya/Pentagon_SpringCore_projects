<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="petrol" class=" carEngine.PetrolEngine"></bean>
    <bean id="diesel" class="carEngine.DieselEngine"></bean>
	<bean id="electric" class="carEngine.ElectricMotor"></bean>
	<bean id="car" class="carEngine.Car">
	
		<!--<constructor-arg name="e" ref="diesel"></constructor-arg>-->
		<property name="e" ref="petrol"></property>
		
	</bean>


</beans>